{
    "compilerOptions": {
      "target": "es5",
      "module": "esnext",
      "jsx": "react-jsx",
      "jsxImportSource": "@emotion/react",
      "moduleResolution": "node",
      "lib": ["es5", "es2016", "dom"],
      "noEmit": false,
      "allowSyntheticDefaultImports": true,
      "typeRoots": [
        "./@types",
        "../node_modules/@types"
      ],
    },
    "include": ["./@types", "**/*.ts"],
    "exclude": [
        "../node_modules"
    ],
  }