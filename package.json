    "@storybook/react": "^8.1.11",
    "@storybook/react-webpack5": "^8.1.11",
    "@storybook/test": "^8.1.11",
    "@svgr/rollup": "8.1.0",
    "@types/cypress": "^1.1.3",
    "@types/node": "^20.14.9",
    "@types/react": "18.3.3",
    "@types/react-dom": "^18.3.0",
    "autoprefixer": "^10.4.19",
    "babel-loader": "^9.1.3",
    "cypress": "^13.12.0",
    "eslint": "8.57.0",
